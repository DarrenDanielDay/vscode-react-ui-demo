{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib": ["es6"],
    "experimentalDecorators": true,
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "skipLibCheck": true,
    "importsNotUsedAsValues": "error",
    "preserveWatchOutput": true
  },
  "exclude": ["node_modules", "src/react-ui/src"]
}
